-- Scripts:

local function DGBUHTK_fake_script() -- close.close 
	local script = Instance.new('LocalScript', close)

	-- LocalScript inside the TextButton
	
	local button = script.Parent
	local frame = button.Parent
	local minimized = false
	local UserInputService = game:GetService("UserInputService")
	
	-- Function to toggle the frame's visibility
	local function toggleFrame()
		if not minimized then
			frame.Visible = false -- Hide the frame
			minimized = true
	
			-- Show notification when frame is hidden
			game.StarterGui:SetCore("SendNotification", {
				Title = "How to re-open without executing again?",
				Text = "Press option on your keyboard to view the UI again",
				Icon = "" -- You can specify an icon here if needed
			})
		else
			frame.Visible = true -- Show the frame
			minimized = false
		end
	end
	
	-- Connect button click to toggleFrame function
	button.MouseButton1Click:Connect(toggleFrame)
	
	-- Function to handle key press
	local function onKeyPress(input, gameProcessed)
		if not gameProcessed then
			if input.KeyCode == Enum.KeyCode.LeftAlt or input.KeyCode == Enum.KeyCode.RightAlt then
				toggleFrame()
			end
		end
	end
	
	-- Connect key press to onKeyPress function
	UserInputService.InputBegan:Connect(onKeyPress)
	
end
coroutine.wrap(DGBUHTK_fake_script)()
local function WSOOUNV_fake_script() -- FovButton.Fov Activate 
	local script = Instance.new('LocalScript', FovButton)

	-- LocalScript inside the Button
	
	local button = script.Parent
	local fov = 100
	local RunService = game:GetService("RunService")
	local Cam = game.Workspace.CurrentCamera
	
	-- Variables for FOV ring
	local FOVring = Drawing.new("Circle")
	FOVring.Visible = false -- Start as invisible
	FOVring.Thickness = 2
	FOVring.Color = Color3.fromRGB(128, 0, 128) -- Purple color
	FOVring.Filled = false
	FOVring.Radius = fov
	FOVring.Position = Cam.ViewportSize / 2
	
	local function updateDrawings()
		local camViewportSize = Cam.ViewportSize
		FOVring.Position = camViewportSize / 2
	end
	
	-- Connect button click to activate the FOV ring
	button.MouseButton1Click:Connect(function()
		FOVring.Visible = true -- Make the FOV ring visible
		RunService.RenderStepped:Connect(updateDrawings) -- Start updating the FOV ring's position
	end)
	
end
coroutine.wrap(WSOOUNV_fake_script)()
local function GMIMV_fake_script() -- HitBoxButton.HitBox Activate 
	local script = Instance.new('LocalScript', HitBoxButton)

	-- LocalScript inside the Button
	
	local button = script.Parent
	
	-- Variables for the script functionality
	_G.HeadSize = 14
	_G.Disabled = true
	
	local function activateHeadResize()
		-- Function that modifies the head size and properties
		game:GetService('RunService').RenderStepped:Connect(function()
			if _G.Disabled then
				for _, player in ipairs(game:GetService('Players'):GetPlayers()) do
					if player.Name ~= game:GetService('Players').LocalPlayer.Name then
						pcall(function()
							local head = player.Character and player.Character:FindFirstChild("Head")
							if head then
								head.Size = Vector3.new(_G.HeadSize, _G.HeadSize, _G.HeadSize)
								head.Transparency = 1
								head.BrickColor = BrickColor.new("Red")
								head.Material = "Neon"
								head.CanCollide = false
								head.Massless = true
							end
						end)
					end
				end
			end
		end)
	end
	
	-- Connect button click to activate the script
	button.MouseButton1Click:Connect(function()
		_G.Disabled = false -- Enable the script when the button is clicked
		activateHeadResize() -- Start the head resizing functionality
	end)
	
end
coroutine.wrap(GMIMV_fake_script)()
local function HGTE_fake_script() -- StaminaButton.Stamina Activate 
	local script = Instance.new('LocalScript', StaminaButton)

	-- LocalScript inside the Button
	
	local button = script.Parent
	
	local function activateScript()
		-- CW | Inf. Stamina & No Jump Cooldown --
		-- Made by shadow--
	
		for i,v in pairs(getgc(true)) do
			if typeof(v) == "table" and rawget(v, "getIsMaxed") then
				v.getIsMaxed = function()
					return false
				end
				v.getFlags = function()
					return 1
				end
				v.addFlags = function(a,b)
					a:setFlags(0)
					return
				end
			end
			if typeof(v) == "table" and rawget(v, "spawnCharacter") then
				local oldfunc = v.spawnCharacter
				v.SpawnCharacter = function(a)
					for _,f in pairs(getgc(true)) do
						if typeof(f) == "table" and rawget(f, "getIsMaxed") then
							f.getIsMaxed = function()
								return false
							end
							f.getFlags = function()
								return 1
							end
							f.addFlags = function(aa,b)
								aa:setFlags(0)
								return
							end
						end
					end
				end
			end
			if typeof(v) == "table" and rawget(v, "getCanJump") then
				v.getCanJump = function()
					return true
				end
			end
			if typeof(v) == "table" and rawget(v, "JUMP_DELAY_ADD") then
				v.JUMP_DELAY_ADD = 0.5
			end
			if typeof(v) == "table" and rawget(v, "_setStamina") then
				v._setStamina = function(a, b)
					a._stamina = math.huge
					a._staminaChangedSignal:Fire(99)
				end
			end
		end
	
		-- Notification to indicate the script has been loaded
		game.StarterGui:SetCore("SendNotification", {Title = "Galactic CW Infinite stamina + no jump cooldown", Text = "Script loaded, "..game.Players.LocalPlayer.DisplayName..".", Duration = 4,})
	end
	
	-- Connect button click to activate the script
	button.MouseButton1Click:Connect(function()
		activateScript()
	end)
	
end
coroutine.wrap(HGTE_fake_script)()
local function WMMFVZW_fake_script() -- Frame.drag script 
	local script = Instance.new('LocalScript', Frame)

	local UserInputService = game:GetService("UserInputService")
	local TweenService = game:GetService("TweenService")
	
	local frame = script.Parent
	local dragging = true
	local dragInput, mousePos, framePos
	
	local function update(input)
		local delta = input.Position - mousePos
		local newPosition = UDim2.new(
			framePos.X.Scale,
			framePos.X.Offset + delta.X,
			framePos.Y.Scale,
			framePos.Y.Offset + delta.Y
		)
		frame.Position = newPosition
	end
	
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			mousePos = input.Position
			framePos = frame.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if dragging and input == dragInput then
			update(input)
		end
	end)
	
end
coroutine.wrap(WMMFVZW_fake_script)()
local function YQLT_fake_script() -- CWRework.AlwaysOnTop Script 
	local script = Instance.new('LocalScript', CWRework)

	local ScreenGui = Instance.new("CW Rework")
	local TopFrame = Instance.new("Frame")
	local BottomFrame = Instance.new("Frame")
	
	ScreenGui.Parent = game.CoreGui
	
	TopFrame.Size = UDim2.new(0, 200, 0, 200)
	TopFrame.Position = UDim2.new(0.5, -100, 0.5, -100)
	TopFrame.BackgroundColor3 = Color3.new(1, 0, 0) -- Red color
	TopFrame.ZIndex = 2 -- Higher ZIndex to be on top
	TopFrame.Parent = ScreenGui
	
	BottomFrame.Size = UDim2.new(0, 200, 0, 200)
	BottomFrame.Position = UDim2.new(0.5, -100, 0.5, 50)
	BottomFrame.BackgroundColor3 = Color3.new(0, 0, 1) -- Blue color
	BottomFrame.ZIndex = 1 -- Lower ZIndex
	BottomFrame.Parent = ScreenGui
	
end
coroutine.wrap(YQLT_fake_script)()
